<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<StatDef>
		<defName>MF_Stat_Module</defName>
		<label>module slot</label>
		<workerClass>Exosuit.StatWorker_Module</workerClass>
		<description>the taken slot of this module, a module might take multiple slots.</description>
		<category>BasicsImportant</category>
	</StatDef>

	<StatDef>
		<defName>MF_Stat_Slot</defName>
		<label>slot user</label>
		<workerClass>Exosuit.StatWorker_SlotUsers</workerClass>
		<description>the modules using this slot, will include module that taken multiple slots.</description>
		<category>BasicsImportant</category>
	</StatDef>

	<StatDef>
		<defName>MF_FlightRange</defName>
		<label>Flight range</label>
		<description>The maximum flight distance.</description>
		<category>Apparel</category>
		<minValue>0</minValue>
		<toStringStyle>FloatOne</toStringStyle>
		<formatString>{0}</formatString>
		<showIfUndefined>false</showIfUndefined>
		<displayPriorityInCategory>59</displayPriorityInCategory>
		<parts>
			<li Class="StatPart_Quality">
				<factorAwful>0.5</factorAwful>
				<factorPoor>0.75</factorPoor>
				<factorNormal>1</factorNormal>
				<factorGood>1.25</factorGood>
				<factorExcellent>1.3</factorExcellent>
				<factorMasterwork>1.4</factorMasterwork>
				<factorLegendary>1.5</factorLegendary>
			</li>
		</parts>
	</StatDef>
</Defs>